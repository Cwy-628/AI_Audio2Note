{
  "name": "ai-audio2note-frontend",
  "version": "1.0.0",
  "description": "AI Audio2Note Frontend - Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "dist": "npm run build",
    "pack": "electron-builder --dir",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "keywords": [
    "electron",
    "audio",
    "video",
    "download"
  ],
  "author": "AI Audio2Note Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.aiaudio2note.app",
    "productName": "AI Audio2Note",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules",
      "!dist"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
