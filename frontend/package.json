{
  "name": "ai-audio2note-frontend",
  "version": "1.0.0",
  "description": "AI Audio2Note Frontend - Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:win": "electron-builder --win --dir",
    "build:mac": "electron-builder --mac --dir",
    "build:linux": "electron-builder --linux --dir",
    "dist": "npm run build",
    "pack": "electron-builder --dir",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "keywords": [
    "electron",
    "audio",
    "video",
    "download"
  ],
  "author": "AI Audio2Note Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.aiaudio2note.app",
    "productName": "AI Audio2Note",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules",
      "!dist"
    ],
    "win": {
      "target": "dir",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dir",
      "icon": "assets/icon.icns",
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": "dir",
      "icon": "assets/icon.png"
    }
  }
}
